<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.multiply.esl_interface.v1.web.mapper.aimscore.AimsCoreMapper">

    <!-- station-code 목록 -->
    <select id="selectStationCodes" parameterType="String" resultType="String">
        SELECT
            code
        FROM
            station
        ORDER BY code ${param}
    </select>

</mapper>
